{
	"name": "directus-extension-increment",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-bundle"
	],
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "endpoint",
				"name": "increment-endpoint",
				"source": "src/increment-endpoint/index.ts"
			},
			{
				"type": "operation",
				"name": "increment-operation",
				"source": {
					"app": "src/increment-operation/app.ts",
					"api": "src/increment-operation/api.ts"
				}
			}
		],
		"host": "^9.21.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"add": "directus-extension add"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "9.21.0",
		"@types/node": "^18.11.9",
		"typescript": "^4.9.3",
		"vue": "^3.2.45"
	}
}
